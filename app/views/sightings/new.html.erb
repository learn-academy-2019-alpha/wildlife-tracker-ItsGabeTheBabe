<h1>Add Sightings</h1>
<%= form_for @sighting do |f| %>
<%= f.label :animal_id %>
<%= f.select :animal_id, options_from_collection_for_select(Animal.all,:id, :common_name) %> </br>
<%= f.label :date%>
<%= f.text_field :date%> </br>

<%= f.label :time%>
<%= f.text_field :time%> </br>

<%= f.label :lat%>
<%= f.text_field :lat%> </br>

<%= f.label :long%>
<%= f.text_field :long%> </br>

<%= f.submit "create"%>
<% end%>